<version>3.11.0</version> 

<configuration> 

<source>1.8</source> 

<target>1.8</target> 

</configuration> 

</plugin> 

 

 

<plugin> 

<groupId>org.apache.maven.plugins</groupId> 

<artifactId>maven-surefire-plugin</artifactId> 

<version>3.2.1</version> 

<configuration> 

<suiteXmlFiles>
